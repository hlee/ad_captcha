# See https://help.github.com/articles/ignoring-files for more about ignoring files.

# Ignore bundler config and downloaded gems
/.bundle
/vendor/bundle/
/vendor/ruby/

# Ignore all logfiles and tempfiles
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore storage (uploaded files in development)
/storage/*
!/storage/.keep
/tmp/storage/*
!/tmp/storage/
!/tmp/storage/.keep

# Ignore master key for decrypting credentials
/config/master.key
/config/credentials/*.key

# Ignore database configuration
/config/database.yml

# Environment files
.env
.env.*
!.env.example

# Ruby/Rails specific
*.rbc
capybara-*.html
.rspec
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-[0-9]*
/coverage/
/spec/tmp/*
*.orig
rerun.txt
pickle-email-*.html

# Node/Yarn specific
node_modules/
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Asset pipeline and webpacker
/public/assets
/public/packs
/public/packs-test
/node_modules
/yarn-error.log
/public/packs
/public/packs-test
/public/assets
/app/assets/builds/*
!/app/assets/builds/.keep

# Ruby version/gemset files
.ruby-version
.ruby-gemset
.rvmrc

# Editor/IDE specific files
/.idea
/.vscode
*.swp
*.swo
*~
.project
.DS_Store
.env
nbproject/
.redcar/
.sass-cache/
*.sublime-workspace
*.sublime-project

# Ignore documentation
/doc/
/.yardoc/
/rdoc/

# Ignore test files
/test/tmp/
/test/version_tmp/

# Ignore Byebug command history file
.byebug_history

# Ignore precompiled javascript packs
/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore uploaded files in development
/storage/*
!/storage/.keep
/public/uploads
